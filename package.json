{
  "name": "next-nightwatch",
  "version": "1.0.0",
  "description": "Integration testing using nightwatch (Selenium) on next.js (react)",
  "repository": "https://github.com/pramendra/next-nightwatch.git",
  "author": "Pramendra Gupta <pramendra@gmail.com>",
  "license": "MIT",
  "main": "server/main.js",
  "scripts": {
    "dev": "node server/main.js",
    "build": "next build",
    "start": "NODE_ENV=production node server/main.js",
    "lint": "eslint '**/*.js' --quiet",
    "format": "prettier --write '**/*.js'",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "test": "nyc ava -T 1m --fast-fail",
    "test:e2e": "nightwatch ./e2e/index.js -e default,mobile"
  },
  "dependencies": {
    "ava": "^1.0.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "next-offline": "^3.3.1",
    "next-redux-wrapper": "^2.1.0",
    "ramda": "^0.26.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.9.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "browser-env": "^3.2.5",
    "chromedriver": "^2.44.1",
    "cssnano": "^4.1.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.89.0",
    "nightwatch": "^1.0.15",
    "nyc": "^13.1.0",
    "postcss-calc": "^7.0.1",
