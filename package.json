{
  "name": "next-nightwatch",
  "version": "1.0.0",
  "description": "Integration testing using nightwatch (Selenium) on next.js (react)",
  "repository": "https://github.com/pramendra/next-nightwatch.git",
  "author": "Pramendra Gupta <pramendra@gmail.com>",
  "license": "MIT",
  "main": "server/main.js",
  "scripts": {
    "dev": "node server/main.js",
    "build": "next build",
    "start": "NODE_ENV=production node server/main.js",
    "test:e2e": "nightwatch ./e2e/index.js -e default,mobile",
    "lint": "eslint '**/*.js' --quiet",
    "format": "prettier --write '**/*.js'",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "dependencies": {
    "ava": "^1.0.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "next": "^7.0.2",
    "next-offline": "^3.3.1",
    "ramda": "^0.26.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.9.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chromedriver": "^2.44.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.89.0",
    "nightwatch": "^1.0.15",
    "prettier": "^1.15.3",
    "selenium-server": "^3.141.59"
